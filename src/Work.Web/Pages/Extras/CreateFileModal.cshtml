@page
@using Work.Localization
@using Work.Web.Pages.Extras
@using Microsoft.Extensions.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model CreateFileModalModel
@inject IStringLocalizer<WorkResource> L
@{
    Layout = null;
}


<abp-dynamic-form abp-model="createUpdateExtra" asp-page="/Extras/CreateFileModal">
    <abp-modal centered="true" size="ExtraLarge">
        <abp-modal-header title="Yeni Dosya"></abp-modal-header>
        <abp-modal-body>
            <abp-form-content />
            <abp-input asp-for="UploadFileDtos.Name"></abp-input>

            <abp-input asp-for="UploadFileDtos.File"></abp-input>
        </abp-modal-body>
      <label id="@Model.result" name="@Model.result" />
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
    
</abp-dynamic-form>